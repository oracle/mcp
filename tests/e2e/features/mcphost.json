{
  "mcpServers": {
    "oracle-oci-api-mcp-server": {
      "disabled": false,
      "timeout": 60,
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "oracle.oci-api-mcp-server"
      ],
      "env": {
        "OCI_CONFIG_FILE": "${env:OCI_CONFIG_FILE}",
        "HTTP_PROXY": "http://127.0.0.1:5000",
        "HTTPS_PROXY": "http://127.0.0.1:5000",
        "OCI_SDK_CERT_BUNDLE": "False",
        "PYTHONHTTPSVERIFY": "0",
        "OCI_SKIP_SSL_VERIFICATION": "True",
        "REQUESTS_CA_BUNDLE": "",
        "CURL_CA_BUNDLE": ""
      }
    },
